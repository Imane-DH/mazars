<h2>Chercher un secteur</h2>
<form action="/secteurs" method="GET">
  <label>Nom</label>
  <input type="text" name="nom" value="<%= searchOptions.nom %>" />
  <button type="submit">Chercher</button>
</form>
<br />
<% secteurs.forEach(secteur => { %>
<div><%= secteur.nom %></div>
<a href="/secteurs/<%= secteur.id %>"> Consulter</a>
<a href="/secteurs/<%= secteur.id %>/edit"> Modifier </a>
<%- include('../partials/deleteForm' , { url: `/secteurs/${pays.id}`}) %> <% })
%>
